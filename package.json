{
  "name": "family-notify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage --forceExit",
    "test:unit": "jest --testPathPattern='__tests__/unit'",
